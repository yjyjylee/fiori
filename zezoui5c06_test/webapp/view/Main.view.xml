<mvc:View controllerName="zezoui5c06test.controller.Main" 
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m" xmlns:f="sap.f">
    <f:DynamicPage toggleHeaderOnTitleClick="false">
        <f:title>
            <f:DynamicPageTitle>
                <f:heading>
                    <Title text="Main View" />
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>

        <f:content>
            <VBox fitContainer="true">
                <Text text="{/zezo_bpSet/Bpcode}"></Text>
                <Table id="productTable" inset="false" items="{/zezo_bpSet}" class="sapFDynamicPageAlignContent" width="auto">
                    <columns>
                        <Column width="12em">
                            <Text text="Product No" />
                            <Text text="Product Name" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem type="Navigation" press=".onListItemPress">
                            <cells>
                                <Text text="{Bpcode}" />
                                <Text text="{Ceoname}" />
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
            </VBox>
        </f:content>
    </f:DynamicPage>
</mvc:View>
